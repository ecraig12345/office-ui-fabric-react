{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["es2015"],
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "lib",
    // "strict": true,
    // "noUnusedLocals": true,
    // "noImplicitAny": true,
    // "noImplicitThis": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "types": ["node"]
  }
}
